<%= simple_form_for(@flat) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title%>
<br/>
  <%= f.label :description %>
  <%= f.text_field :description%>

<br/>

  <%= f.label :address %>
  <%= f.text_field :address%>
<br/>
  <%= f.label :price %>
  <%= f.text_field :price%>
<br/>
  <%= f.label :capacity %>
  <%= f.text_field :capacity%>
<br/>
  <%= f.label :start_date %>
  <%= f.text_field :start_date%>
<br/>
  <%= f.label :end_date %>
  <%= f.text_field :end_date%>
<br/>
<%= f.input :photo %>
<%= f.input :photo_cache, as: :hidden %>

<br/>

<%= f.submit class: 'btn btn-primary' %>

<% end %>


 <%= link_to("Back to index", flats_path)%>
